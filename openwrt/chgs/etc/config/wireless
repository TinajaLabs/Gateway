
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option macaddr '00:0f:02:20:00:7e'
	option hwmode '11ng'
	option htmode 'HT40-'
	list ht_capab 'GF'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC12'
	option noscan '1'
	option txpower '0'
	option country 'US'

config wifi-iface
	option device 'radio0'
	option key 'Stanf228'
	option network 'wan'
	option encryption 'psk-mixed'
	option mode 'ap'
	option ssid 'TinajaLabs'

